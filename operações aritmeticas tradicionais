#include <iostream>
#include<stdlib.h>
#include<stdio.h>



int main(int argc, char** argv) {
	
		int a,b;
		printf("Introduza 2 inteiros: ");
		scanf("%d%d",&a,&b);				//recebe os valores.
		printf("%d+%d = %d\n",a,b,a+b);     //soma.
		printf("%d-%d = %d\n",a,b,a-b);		//subtrai.
		printf("%d*%d = %d\n",a,b,a*b);		//multiplica.
		printf("%d/%d = %d\n",a,b,a/b);		//divide.
		printf("%d %% %d = %d\n",a,b,a%b);  //Modulo
			
	
	
	return 0;
}
